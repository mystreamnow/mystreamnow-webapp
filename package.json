    "redux-react-session": "^2.3.1",
    "redux-saga": "^1.0.5",
    "redux-thunk": "^2.2.0",
    "scriptjs": "^2.5.9",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
