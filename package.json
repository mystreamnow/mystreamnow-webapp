    "react-apollo": "^2.5.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "^5.0.0",
    "react-display-name": "^0.2.4",
    "react-dom": "^16.8.6",
    "react-full-screen": "^0.2.4",
    "react-helmet": "^5.2.1",
    "react-localize-redux": "^2.16.0",
    "react-pusher": "^0.2.0",
    "react-rating": "^1.7.2",
